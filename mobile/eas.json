{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "bundleIdentifier": "com.bolaxo.app"
      },
      "android": {
        "package": "com.bolaxo.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-email@bolaxo.se",
        "ascAppId": "auto",
        "appleTeamId": "auto"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      }
    }
  }
}

