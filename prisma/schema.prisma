generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Valuation {
  id           String   @id @default(cuid())
  createdAt    DateTime @default(now())
  email        String?
  companyName  String?
  industry     String?
  inputJson    Json
  resultJson   Json
  mostLikely   Int
  minValue     Int
  maxValue     Int
}
